#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Temp/Github/nw_astro/ARK-Ascended-Server-Manager-Astro/node_modules/.pnpm/astro@5.16.9_@types+node@25_6c89909294629f9aa9c0038bfd80cfcb/node_modules/astro/node_modules:/mnt/c/Temp/Github/nw_astro/ARK-Ascended-Server-Manager-Astro/node_modules/.pnpm/astro@5.16.9_@types+node@25_6c89909294629f9aa9c0038bfd80cfcb/node_modules:/mnt/c/Temp/Github/nw_astro/ARK-Ascended-Server-Manager-Astro/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Temp/Github/nw_astro/ARK-Ascended-Server-Manager-Astro/node_modules/.pnpm/astro@5.16.9_@types+node@25_6c89909294629f9aa9c0038bfd80cfcb/node_modules/astro/node_modules:/mnt/c/Temp/Github/nw_astro/ARK-Ascended-Server-Manager-Astro/node_modules/.pnpm/astro@5.16.9_@types+node@25_6c89909294629f9aa9c0038bfd80cfcb/node_modules:/mnt/c/Temp/Github/nw_astro/ARK-Ascended-Server-Manager-Astro/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
