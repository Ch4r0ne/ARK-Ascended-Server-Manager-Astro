#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Temp/Github/nw_astro/ARK-Ascended-Server-Manager-Astro/node_modules/.pnpm/vite@6.4.1_@types+node@25.0_2a6703aaebdca6542462dd5775fceb0d/node_modules/vite/bin/node_modules:/mnt/c/Temp/Github/nw_astro/ARK-Ascended-Server-Manager-Astro/node_modules/.pnpm/vite@6.4.1_@types+node@25.0_2a6703aaebdca6542462dd5775fceb0d/node_modules/vite/node_modules:/mnt/c/Temp/Github/nw_astro/ARK-Ascended-Server-Manager-Astro/node_modules/.pnpm/vite@6.4.1_@types+node@25.0_2a6703aaebdca6542462dd5775fceb0d/node_modules:/mnt/c/Temp/Github/nw_astro/ARK-Ascended-Server-Manager-Astro/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Temp/Github/nw_astro/ARK-Ascended-Server-Manager-Astro/node_modules/.pnpm/vite@6.4.1_@types+node@25.0_2a6703aaebdca6542462dd5775fceb0d/node_modules/vite/bin/node_modules:/mnt/c/Temp/Github/nw_astro/ARK-Ascended-Server-Manager-Astro/node_modules/.pnpm/vite@6.4.1_@types+node@25.0_2a6703aaebdca6542462dd5775fceb0d/node_modules/vite/node_modules:/mnt/c/Temp/Github/nw_astro/ARK-Ascended-Server-Manager-Astro/node_modules/.pnpm/vite@6.4.1_@types+node@25.0_2a6703aaebdca6542462dd5775fceb0d/node_modules:/mnt/c/Temp/Github/nw_astro/ARK-Ascended-Server-Manager-Astro/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@6.4.1_@types+node@25.0_2a6703aaebdca6542462dd5775fceb0d/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@6.4.1_@types+node@25.0_2a6703aaebdca6542462dd5775fceb0d/node_modules/vite/bin/vite.js" "$@"
fi
